{
  "name": "@gelato-ui/components",
  "version": "1.0.0",
  "private": true,
  "description": "Gelato UI Components Library",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./components/*": {
      "import": "./src/components/*.tsx",
      "require": "./src/components/*.tsx",
      "types": "./src/components/*.tsx"
    },
    "./components/ui/*": {
      "import": "./src/components/ui/*.tsx",
      "require": "./src/components/ui/*.tsx",
      "types": "./src/components/ui/*.tsx"
    },
    "./components/wallet/*": {
      "import": "./src/components/wallet/*.tsx",
      "require": "./src/components/wallet/*.tsx",
      "types": "./src/components/wallet/*.tsx"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "require": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts",
      "require": "./src/utils/*.ts",
      "types": "./src/utils/*.ts"
    },
    "./lib/utils": {
      "import": "./src/lib/utils.ts",
      "require": "./src/lib/utils.ts",
      "types": "./src/lib/utils.ts"
    },
    "./styles": {
      "import": "./src/styles/index.css",
      "require": "./src/styles/index.css"
    },
    "./styles/*": {
      "import": "./src/styles/*.css",
      "require": "./src/styles/*.css"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0-alpha",
    "next-themes": "^0.4.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.344.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}