{
  "name": "@gelato-ui/components",
  "version": "1.0.0",
  "private": true,
  "description": "Gelato UI Components Library",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./components/*": {
      "import": "./src/components/*.tsx",
      "require": "./src/components/*.tsx",
      "types": "./src/components/*.tsx"
    },
    "./components/ui/*": {
      "import": "./src/components/ui/*.tsx",
      "require": "./src/components/ui/*.tsx",
      "types": "./src/components/ui/*.tsx"
    },
    "./components/wallet/*": {
      "import": "./src/components/wallet/*.tsx",
      "require": "./src/components/wallet/*.tsx",
      "types": "./src/components/wallet/*.tsx"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "require": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts",
      "require": "./src/utils/*.ts",
      "types": "./src/utils/*.ts"
    },
    "./lib/utils": {
      "import": "./src/lib/utils.ts",
      "require": "./src/lib/utils.ts",
      "types": "./src/lib/utils.ts"
    },
    "./styles": {
      "import": "./src/styles/index.css",
      "require": "./src/styles/index.css"
    },
    "./styles/*": {
      "import": "./src/styles/*.css",
      "require": "./src/styles/*.css"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0-alpha"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.344.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  }
}